<% unless @preview %>
  $("#<%= dom_id(@track) %>").remove();
  $('#track_uploaded_data_inputUploader').css('visibility', 'visible');
  trackCount -= 1;
  uploadedTrackCount -= 1;
<% else %>
  $("#<%= dom_id(@track) %>").remove();
  $('#upload_preview_button').show();
  previewUploadInProgress = false;
<% end %>

updateUploadOriginal();
refreshUploader();
